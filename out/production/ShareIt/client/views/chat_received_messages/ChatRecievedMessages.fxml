<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.chat_received_messages.ChatReceivedMessagesController">
   <content>
      <AnchorPane prefHeight="600.0" prefWidth="1280.0">
         <children>
            <VBox layoutY="90.0" maxWidth="1.7976931348623157E308" prefHeight="485.0" prefWidth="1280.0" style="-fx-background-color: #FF665A;">
               <padding>
                  <Insets bottom="20.0" left="160.0" right="160.0" top="20.0" />
               </padding>
               <children>
                  <Label text="Recieved messages" textFill="WHITE">
                     <font>
                        <Font name="Californian FB" size="24.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="30.0" />
                     </VBox.margin>
                  </Label>
                  <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" />
               </children>
            </VBox>
            <Button layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#onGoBack" style="-fx-border-radius: 30px; -fx-background-radius: 30px; -fx-background-color: #A3A1A8; -fx-border-color: #A3A1A8;" text="Go back to user's page" textFill="WHITE">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
