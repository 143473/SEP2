<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.chat_received_messages.ChatReceivedMessagesController">
   <content>
      <AnchorPane prefHeight="600.0" prefWidth="1280.0">
         <children>
            <VBox layoutY="90.0" maxWidth="1.7976931348623157E308" prefHeight="485.0" prefWidth="1280.0" style="-fx-background-color: #FF665A;">
               <padding>
                  <Insets bottom="20.0" left="160.0" right="160.0" top="20.0" />
               </padding>
               <children>
                  <HBox>
                     <children>
                        <VBox prefHeight="200.0" prefWidth="550.0">
                           <children>
                              <Label text="Received messages" textFill="WHITE">
                                 <font>
                                    <Font name="Californian FB" size="24.0" />
                                 </font>
                              </Label>
                              <VBox fx:id="vBoxRight" />
                           </children>
                        </VBox>
                        <VBox prefWidth="479.0">
                           <children>
                              <Label text="Received warnings" textFill="#010101">
                                 <font>
                                    <Font name="Californian FB" size="24.0" />
                                 </font>
                              </Label>
                              <VBox fx:id="vBoxLeft" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
